{
  "name": "tarball-unpacker",
  "version": "1.0.0",
  "description": "Tarball unpacker tool for Node.js",
  "main": "unpacker.js",
  "scripts": {
    "test": "mocha test/**/*-test.js",
    "lint": "jslint '*.js' 'test/**/*.js'",
    "coverage": "istanbul cover _mocha test/unit/**/*.js",
    "travis": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --report lcovonly -- -R spec test/**/*-test.js && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arkaitzgarro/tarball-unpacker.git"
  },
  "keywords": [
    "tarball",
    "gzip",
    "gz",
    "node",
    "decompress",
    "decompression"
  ],
  "author": "Arkaitz Garro <arkaitz.garro@gmail.com>",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/arkaitzgarro/tarball-unpacker/issues"
  },
  "bin": {
    "unpacker": "bin/cli.js"
  },
  "homepage": "https://github.com/arkaitzgarro/tarball-unpacker#readme",
  "dependencies": {
    "bluebird": "^3.0.6",
    "tar": "^2.2.1",
    "yargs": "^3.31.0"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "istanbul": "^0.4.1",
    "jslint": "^0.9.5",
    "mocha": "^2.1.0",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0"
  },
  "engines": {
    "node": ">= 0.12.7"
  }
}
